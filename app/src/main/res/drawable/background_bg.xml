<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:start="@dimen/_1sdp">
        <shape android:shape="rectangle">
            <solid android:color="@color/dark_black"/>
            <corners  android:bottomLeftRadius="@dimen/_10sdp"
                android:bottomRightRadius="@dimen/_10sdp"/>
        </shape>
    </item>

    <item
        android:bottom="@dimen/_3sdp"
        android:end="@dimen/_1sdp">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_gugd_accent"/>
            <corners android:bottomLeftRadius="@dimen/_10sdp" android:bottomRightRadius="@dimen/_10sdp"/>
            <stroke android:color="@color/black"
                android:width="1dp"/>
        </shape>
    </item>
    <item
        android:height="1dp"
        android:start="1dp"
        android:bottom="@dimen/_3sdp"
        android:end="@dimen/_4sdp">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_gugd_accent"/>

        </shape>
    </item>


</layer-list>